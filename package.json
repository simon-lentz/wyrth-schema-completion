{
    "name": "test-extension",
    "displayName": "test-extension",
    "description": "Simon's test extension for VS Code",
    "version": "0.0.1",
    "publisher": "simon-lentz",
    "repository": "https://github.com/simon-lentz/test-extension",
    "engines": {
      "vscode": "^1.51.0"
    },
    "categories": ["Other"],
